    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.15.3",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "zod": "^4.1.5"
  },
